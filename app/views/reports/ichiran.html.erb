<div class="mx-0 mt-5 pt-4">
  <% if @report.title.present? %>
      <h4 class="d-none d-sm-block mb-0 py-4 px-2 report_title"><%= @report.title %></h4>
      <h5 class="d-block d-sm-none mx-1 py-3 px-2 report_title"><%= @report.title %></h5>
  <% end %>
  <div class="px-2 pt-3">
    <%= @report.content %>
  </div>
</div>

<div class="cp_box mt-5">
  <input id="cp01" type="checkbox">
  <label for="cp01"></label>
  <div class="cp_container">
    <table class="table table-hover treatment-all-list">
      <thead>
        <tr>
          <th class="text-center matome-title" colspan="2" style="color:#1A2438"><%= @report.user.name %>&nbsp;<span class="text-muted">さんの</span>&nbsp;<%= @report.clinic.name %>&nbsp;<span class="text-muted">での</span>&nbsp;<span class="font-weight-bold">治療情報一覧</span></th>
        </tr>
        <tr>
          <td class="text-center result-left-column" style="width:35%">項目</td>
          <td class="text-center" style="width:65%">内容</td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td class="result-left-column small">投稿時の状況</td>
          <% if @report.current_state.present? %>
            <td class="pl-4 result-right-column"><%= @report.str_current_state %></td>
          <% else %>
            <td class="pl-4 result-right-column"><small class="text-muted">未回答</small></td>
          <% end %>
        </tr>
        <tr>
          <td class="result-left-column small">クリニック</td>
          <td class="pl-4 result-right-column"><%= @report.clinic.name %></td>
        </tr>
        <tr>
          <td class="result-left-column small">治療当時の住まい&nbsp;<sup class="text-success">●</sup></td>
          <td class="pl-4 result-right-column">
            <% if @report.prefecture_at_the_time_status == "hide" %>
              <span class="small text-muted">&nbsp;&nbsp;非公開 <span class="small">(都道府県)</span></span>
            <% else %>
              <span class="result-text"><%= @report.prefecture.name %></span>&nbsp;&nbsp;
            <% end %>
            <% if @report.city_at_the_time_status == "hide" %>
              <span class="small text-muted">&nbsp;&nbsp;非公開 <span class="small">(市区町村)</span></span>
            <% else %>
              <span class="result-text"><%= @report.city.name %></span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">何院目</td>
          <td class="pl-4 result-right-column">
            <% if @report.number_of_clinics.present? %>
              <span class="result-text"><%= @report.str_number_of_clinics %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %><br>
            <% if @clinics.present? %><span class="small text-muted">(</span>
              <% @clinics.each do |c| %>
                <span class="result-text small"><%= c %></span>&nbsp;&nbsp;<span class="small text-muted mb-0">→&nbsp;</span>
              <% end %>
              <span class="result-text small"><%= @report.clinic.name %></span><span class="small text-muted">&nbsp;)</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">CL選定理由</td>
          <td class="pl-4 result-right-column">
            
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">説明会への参加有無</td>
          <% if @report.briefing_session.present? %>
            <td class="pl-4 result-right-column"><%= @report.str_briefing_session %></td>
          <% else %>
            <td class="pl-4 result-right-column"><small class="text-muted">未回答</small></td>
          <% end %>
        </tr>
        <tr>
          <td class="result-left-column small">何人目治療</td>
          <td class="pl-4 result-right-column">
            <% if @report.fertility_treatment_number.present? %>
              <span class="result-text"><%= @report.str_fertility_treatment_number %>治療</span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
            <td class="result-left-column small">卵子と精子</td>
            <td class="pl-4">
              <% if @report.types_of_eggs_and_sperm.present? %>
                <span class="result-text"><%= @report.str_types_of_eggs_and_sperm %></span>
              <% else %>
                <span class="text-muted small">未回答</span>
              <% end %><br><br>
              <% if @report.description_of_eggs_and_sperm_used.present? %>
                <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.description_of_eggs_and_sperm_used.split("\n"),tag(:br)))).html_safe %></span>
              <% end %>
            </td>
          </tr>
        <tr>
          <td class="result-left-column small">受精方法</td>
          <td class="pl-4 result-right-column">
            <% if @report.types_of_fertilization_methods.present? %>
              <span class="result-text"><%= @report.str_types_of_fertilization_methods %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <% if @report.types_of_fertilization_methods != 1 %>
          <tr>
            <td class="result-left-column small">顕微授精の種類</td>
            <td class="pl-4">
              <% if @report.details_of_icsi.present? %>
                <span class="result-text"><%= @report.str_details_of_icsi %></span>
              <% else %>
                <span class="text-muted small">未回答</span>
              <% end %>
            </td>
          </tr>
        <% end %>
        <tr>
          <td class="result-left-column small">移植方法</td>
          <td class="pl-4 result-right-column">
            <% if @report.transplant_method.present? %>
              <span class="result-text"><%= @report.str_transplant_method %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">妊活/不妊治療の開始年齢</td>
          <td class="pl-4 result-right-column">
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">このCLでの治療開始時年齢</td>
          <td class="pl-4 result-right-column">
            <% if @report.treatment_start_age.present? %>
              <span class="result-text"><%= @report.str_treatment_start_age %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">このCLでの治療終了時年齢</td>
          <td class="pl-4 result-right-column">
            <% if @report.year_of_treatment_end.present? %>
              <span class="result-text"><%= @report.year_of_treatment_end.year %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">このCLでの治療終了時のパートナーの年齢</td>
          <td class="pl-4 result-right-column">
            <% if @report.age_of_partner_at_end_of_treatment.present? %>
              <span class="result-text"><%= @report.str_age_of_partner_at_end_of_treatment %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">このCLでの治療終了年</td>
          <td class="pl-4 result-right-column">
            <% if @report.year_of_treatment_end.present? %>
              <span class="result-text"><%= @report.year_of_treatment_end.year %>年</span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">通院期間</td>
          <td class="pl-4 result-right-column">
            <% if @report.treatment_period.present? %>
              <span class="result-text"><%= @report.str_treatment_period %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">人工授精の回数</td>
          <td class="pl-4 result-right-column">
          </td>
        </tr>
        
        <tr>
          <td class="result-left-column small">不育症診断</td>
          <td class="pl-4">
            <% if @report.fuiku.present? %>
              <span class="result-text"><%= @report.str_fuiku %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <% if @report.fuiku_supplementary_explanation.present? %>
            <td class="result-left-column small">流産や不育症について</td>
            <td class="pl-4">
              <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i><%= text_url_to_link(h(safe_join(@report.fuiku_supplementary_explanation.split("\n"),tag(:br)))).html_safe %></span>
            </td>
          <% end %>
        </tr>
        <tr>
          <td class="result-left-column small">養子縁組について</td>
          <td class="pl-4 result-right-column">
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">AMH</td>
          <td class="pl-4 result-right-column">
            <% if @report.amh.present? %>
              <span class="result-text"><%= @report.str_amh %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">喫煙有無</td>
          <td class="pl-4 result-right-column">
            
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">女性側の不妊原因</td>
          <td class="pl-4 result-right-column">
            <% if @report.f_infertility_factors.present? %>
              <% @report.f_infertility_factors.each do |f_infertility_factor| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= f_infertility_factor.name %></li>
                </ul>
              <% end %>
            <% else %>
                <span class="text-muted small">なし</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">男性側の不妊原因</td>
          <td class="pl-4 result-right-column">
            <% if @report.m_infertility_factors.present? %>
              <% @report.m_infertility_factors.each do |m_infertility_factor| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= m_infertility_factor.name %></li>
                </ul>
              <% end %>
            <% else %>
                <span class="text-muted small">なし</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">女性側の疾患</td>
          <td class="pl-4 result-right-column">
            <% if @report.f_diseases.present? %>
              <% @report.f_diseases.each do |f_disease| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= f_disease.name %></li>
                </ul>
              <% end %>
            <% else %>
              <span class="text-muted small">なし</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">男性側の疾患</td>
          <td class="pl-4 result-right-column">
            <% if @report.m_diseases.present? %>
              <% @report.m_diseases.each do |m_disease| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= m_disease.name %></li>
                </ul>
              <% end %>
            <% else %>
              <span class="text-muted small">なし</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">女性側の手術既往歴</td>
          <td class="pl-4 result-right-column">
            <% if @report.f_surgeries.present? %>
              <% @report.f_surgeries.each do |f_surgery| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= f_surgery.name %></li>
                </ul>
              <% end %>
            <% else %>
                <span class="text-muted small">なし</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">男性側の手術既往歴</td>
          <td class="pl-4 result-right-column">
            <% if @report.m_surgeries.present? %>
              <% @report.m_surgeries.each do |m_surgery| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= m_surgery.name %></li>
                </ul>
              <% end %>
            <% else %>
                <span class="text-muted small">なし</span>
            <% end %>
          </td>
        </tr>
        <% if @report.f_infertility_memo.present? %>
          <tr>
            <td class="result-left-column small">不妊原因/疾患/手術などについて</td>
            <td class="pl-4 result-right-column">
              <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i><%= text_url_to_link(h(safe_join(@report.f_infertility_memo.split("\n"),tag(:br)))).html_safe %></span>
            </td>
          </tr>
        <% end %>
        <tr>
          <td class="result-left-column small">採卵時の年齢</td>
          <td class="pl-3">
            <% if @report.sairan_age.present? %>
              <span class="result-text"><%= @report.str_sairan_age %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">移植時の年齢</td>
          <td class="pl-3">
            <% if @report.ishoku_age.present? %>
              <span class="result-text"><%= @report.str_ishoku_age %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">採卵誘発タイプ</td>
          <td class="pl-4 result-right-column">
            <% if @report.type_of_ovarian_stimulation.present? %>
              <span class="result-text"><%= @report.str_type_of_ovarian_stimulation %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">採卵誘発方法</td>
          <td class="pl-4 result-right-column">
            <% if @report.type_of_sairan_cycle.present? %>
              <span class="result-text"><%= @report.str_type_of_sairan_cycle %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %><br><br>
            <% if @report.notes_on_type_of_sairan_cycle.present? %>
              <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.notes_on_type_of_sairan_cycle.split("\n"),tag(:br)))).html_safe %></span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">卵巣刺激(〜採卵周期)での使用薬剤</td>
          <td class="pl-4 result-right-column">
            <% if @report.sairan_medicines.present? %>
              <% @report.sairan_medicines.each do |sairan_medicine| %>
                <ul class="list-unstyled">
                  <li class="result-text small"><%= sairan_medicine.name %></li>
                </ul>
              <% end %>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">採卵時の麻酔の種類</td>
          <td class="pl-4 result-right-column">
            <% if @report.use_of_anesthesia.present? %>
              <span class="result-text"><%= @report.str_use_of_anesthesia %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">麻酔の有無や種類の選択</td>
          <td class="pl-4 result-right-column">
            <% if @report.selection_of_anesthesia_type.present? %>
              <span class="result-text"><%= @report.str_selection_of_anesthesia_type %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">採卵日とホルモン値</td>
          <td class="pl-4">
          <% if @report.day_of_sairans.present? %>
            <% @report.day_of_sairans.each do |day_of_sairan| %>
              <table class="table-borderless">
                <tr>
                  <td class="small text-muted text-right">Day&nbsp;:</td>
                  <td class="font-weight-bold">
                    <%= day_of_sairan.day %>
                  </td>
                </tr>
                <tr>
                  <td class="small text-muted text-right">E2&nbsp;:</td>
                  <% if day_of_sairan.e2.blank? %>
                    <td class="font-weight-bold">
                      <span class="text-muted small">未回答</span>
                    </td>
                  <% else %>
                    <td class="font-weight-bold">
                      <%= day_of_sairan.e2 %>&nbsp;<span class="small text-muted"><small>pg/ml</small></span>
                    </td>
                  <% end %>
                </tr>
                <tr>
                  <td class="small text-muted text-right">FSH&nbsp;:</td>
                  <% if day_of_sairan.fsh.blank? %>
                    <td class="font-weight-bold">
                      <span class="text-muted small">未回答</span>
                    </td>
                  <% else %>
                    <td class="font-weight-bold">
                      <%= day_of_sairan.fsh %>&nbsp;<span class="small text-muted"><small>mIU/ml</small></span>
                    </td>
                  <% end %>
                </tr>
                <tr>
                  <td class="small text-muted text-right">LH&nbsp;:</td>
                  <% if day_of_sairan.lh.blank? %>
                    <td class="font-weight-bold">
                      <span class="text-muted small">未回答</span>
                  <% else %>
                    <td class="font-weight-bold">
                      <%= day_of_sairan.lh %>&nbsp;<span class="small text-muted"><small>mIU/ml</small></span>
                    </td>
                  <% end %>
                </tr>
                <tr>
                  <td class="small text-muted text-right">P4&nbsp;:</td>
                  <% if day_of_sairan.p4.blank? %>
                    <td class="font-weight-bold">
                      <span class="text-muted small">未回答</span>
                  <% else %>
                    <td class="font-weight-bold">
                      <%= day_of_sairan.p4 %>&nbsp;<span class="small text-muted"><small>ng/ml</small></span>
                    </td>
                  <% end %>
                </tr>
              </table>
            <% end %>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">精液検査</td>
          <td class="pl-4">
            <table class="table-borderless">
              <tr>
                <td class="small text-muted text-right">精液量&nbsp;:</td>
                <% if @report.semen_volume.blank? %>
                <td class="font-weight-bold">
                  <span class="text-muted small">未回答</span>
                  </td>
                <% else %>
                  <td class="font-weight-bold">
                    <%= @report.semen_volume %>&nbsp;&nbsp;<span class="small text-muted"><small>ml</small></span>
                  </td>
                <% end %>
              </tr>
              <tr>
                <td class="small text-muted text-right">総精子数&nbsp;:</td>
                <% if @report.total_amount_of_sperm.blank? %>
                  <td class="font-weight-bold">
                    <span class="text-muted small">未回答</span>
                  </td>
                <% else %>
                  <td class="font-weight-bold">
                    <%= @report.total_amount_of_sperm.to_s(:delimited) %>&nbsp;&nbsp;<span class="small text-muted"><small>/&nbsp;ml</small></span>
                  </td>
                <% end %>
              </tr>
              <tr>
                <td class="small text-muted text-right">精子濃度&nbsp;:</td>
                <% if @report.semen_concentration.blank? %>
                  <td class="font-weight-bold">
                    <span class="text-muted small">未回答</span>
                  </td>
                <% else %>
                  <td class="font-weight-bold">
                    <%= @report.semen_concentration.to_s(:delimited) %>&nbsp;&nbsp;<span class="small text-muted"><small>/&nbsp;ml</small></span>
                  </td>
                <% end %>
              </tr>
              <tr>
                <td class="small text-muted text-right">運動率&nbsp;:</td>
                <% if @report.sperm_motility.blank? %>
                  <td class="font-weight-bold">
                    <span class="text-muted small">未回答</span>
                <% else %>
                  <td class="font-weight-bold">
                    <%= @report.sperm_motility %>&nbsp;&nbsp;<span class="small text-muted"><small>%</small></span>
                  </td>
                <% end %>
              </tr>
              <tr>
                <td class="small text-muted text-right">前進運動率&nbsp;:</td>
                <% if @report.sperm_advance_rate.blank? %>
                  <td class="font-weight-bold">
                    <span class="text-muted small">未回答</span>
                <% else %>
                  <td class="font-weight-bold">
                    <%= @report.sperm_advance_rate %>&nbsp;&nbsp;<span class="small text-muted"><small>%</small></span>
                  </td>
                <% end %>
              </tr>
              <tr>
                <td class="small text-muted text-right">正常形態率&nbsp;:</td>
                <% if @report.probability_of_normal_morphology_of_sperm.blank? %>
                  <td class="font-weight-bold">
                    <span class="text-muted small">未回答</span>
                <% else %>
                  <td class="font-weight-bold">
                    <%= @report.probability_of_normal_morphology_of_sperm %>&nbsp;&nbsp;<span class="small text-muted"><small>%</small></span>
                  </td>
                <% end %>
              </tr>
            </table>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small">精液検査メモ</td>
          <td class="pl-3">
            <% if @report.sperm_description.present? %>
              <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.sperm_description.split("\n"),tag(:br)))).html_safe %></span>
            <% else %>
