<div class="cp_box mt-0">
  <input id="cp01" type="checkbox">
  <label for="cp01"></label>
  <div class="cp_container">
    <div class="text-center matome-title mb-3 pt-3">
      <%= link_to @report.user.name, user_path(@report.user_id) %>&nbsp;<span class="text-muted small">さんの</span>&nbsp;
      <%= link_to @report.clinic.name, "/category/clinics/#{@report.clinic.id}" %><br>
      <span class="text-muted small">での</span>&nbsp;<span class="font-weight-bold">治療情報 一括表示</span>
    </div>

    <table class="table table-hover p-0 mt-0">
      <tr>
        <td class="result-left-column" colspan="2">
          <div class="m-0 p-0 form-inline justify-content-center">
            <p class="m-0 p-0 text-muted small">治療</p>&nbsp;&nbsp;<i data-feather="chevrons-right" class="feather-icon-style1"></i>&nbsp;&nbsp;<h6 class="m-0 p-0 font-weight-bold">概要</h6>
          </div>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center ichirantable-left">ステータス</td>
        <td class="pl-3 ichirantable-right">
          <% if @report.current_state.present? %>
            <span class="result-text"><%= @report.str_current_state %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">何人目</td>
        <td class="pl-3">
          <% if @report.fertility_treatment_number.present? %>
            <span class="result-text"><%= @report.str_fertility_treatment_number %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">通院開始</td>
        <td class="pl-3">
          <% if @report.treatment_start_age.present? %>
            <span class="result-text"><%= @report.str_treatment_start_age %></span><br>
          <% else %>
            <span class="text-muted small">未回答</span><br>
          <% end %>
          <% if @report.user.first_age_to_start %>
            <small>
              <span class="small text-muted">妊活自体は&nbsp;</span><%= @report.user.str_first_age_to_start(@report.user.first_age_to_start) %><span class="small text-muted">&nbsp;から開始</span>
            </small><br>
          <% end %>
          <% if @report.user.first_age_to_start_art %>
            <small>
              <span class="small text-muted">高度治療は&nbsp;</span><%= @report.user.str_first_age_to_start_art(@report.user.first_age_to_start_art) %><span class="small text-muted">&nbsp;から開始</span>
            </small>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">通院終了</td>
        <td class="pl-3">
          <% if @report.treatment_end_age.present? %>
            <span class="result-text"><%= @report.str_treatment_end_age %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
          <% if @report.year_of_treatment_end.present? %>
            <span class="result-text small">(<%= @report.year_of_treatment_end.year %>年卒業)</span><br>
          <% end %>
          <% if @report.age_of_partner_at_end_of_treatment.present? %>
            <span class="result-text">パートナー:&nbsp;<%= @report.str_age_of_partner_at_end_of_treatment %></span><br>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">通院期間</td>
        <td class="pl-3">
          <% if @report.treatment_period.present? %>
            <span class="result-text"><%= @report.str_treatment_period %></span><br>
          <% else %>
            <span class="text-muted small">未回答</span><br>
          <% end %>
          <% if @report.treatment_period && @report.rest_period %>
            <% if @report.rest_period === 0 %>
              <small>
                <span class="small text-muted">お休み期間&nbsp;</span><%= @report.str_rest_period %><span class="small text-muted"></span>
              </small>
            <% elsif @report.rest_period < 100 %>
              <small>
                <span class="small text-muted">内お休み期間&nbsp;</span><%= @report.str_rest_period %><span class="small text-muted">&nbsp;あり</span>
              </small>
            <% else %>
            <% end %>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">卵子と精子①</td>
        <td class="pl-3">
          <% if @report.types_of_eggs_and_sperm.present? %>
            <span class="result-text"><%= @report.str_types_of_eggs_and_sperm %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %><br><br>
          <% if @report.description_of_eggs_and_sperm_used.present? %>
            <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.description_of_eggs_and_sperm_used.split("\n"),tag(:br)))).html_safe %></span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">卵子と精子②</td>
        <td class="pl-3">
          <% if @report.types_of_eggs_and_sperm_status.present? %>
            <span class="result-text"><%= @report.str_types_of_eggs_and_sperm_status %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %><br><br>
          <% if @report.description_of_eggs_and_sperm_used_status.present? %>
            <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.description_of_eggs_and_sperm_used_status.split("\n"),tag(:br)))).html_safe %></span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">受精方法</td>
        <td class="pl-3">
          <% if @report.types_of_fertilization_methods.present? %>
            <span class="result-text"><%= @report.str_types_of_fertilization_methods %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %><br><br>
          <% if @report.types_of_fertilization_methods_memo.present? %>
            <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.types_of_fertilization_methods_memo.split("\n"),tag(:br)))).html_safe %></span>
          <% end %>
        </td>
      </tr>
      <% if @report.types_of_fertilization_methods === 2 || 3 %>
        <tr>
          <td class="result-left-column small text-center">顕微授精の種類</td>
          <td class="pl-3">
            <% if @report.details_of_icsi.present? %>
              <span class="result-text"><%= @report.str_details_of_icsi %></span>
            <% else %>
              <span class="text-muted small">未回答</span>
            <% end %><br><br>
            <% if @report.details_of_icsi_memo.present? %>
              <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.details_of_icsi_memo.split("\n"),tag(:br)))).html_safe %></span>
            <% end %>
          </td>
        </tr>
      <% end %>
      <tr>
        <td class="result-left-column small text-center">移植方法</td>
        <td class="pl-3">
          <% if @report.transplant_method.present? %>
            <span class="result-text"><%= @report.str_transplant_method %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">費用</td>
        <td class="pl-3">
          <% if @report.cost.present? %>
            <span class="result-text"><%= @report.str_cost %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
    </table>

    <table class="table table-hover p-0 mt-5">
      <tr>
        <td class="result-left-column" colspan="2">
          <div class="m-0 p-0 form-inline justify-content-center">
            <p class="m-0 p-0 text-muted small">治療</p>&nbsp;&nbsp;<i data-feather="chevrons-right" class="feather-icon-style1"></i>&nbsp;&nbsp;<h6 class="m-0 p-0 font-weight-bold">検査</h6>
          </div>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center ichirantable-left">AMH</td>
        <td class="pl-3 ichirantable-right">
          <% if @report.amh.present? %>
            <span class="result-text"><%= @report.str_amh %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">基本検査</td>
        <td class="pl-3">
          <span class="small text-muted"><i class="fas fa-female fa-2x female-icon"></i></span><br>
          <% if @report.cl_female_inspections.present? %>
            <% @report.cl_female_inspections.each do |cl_female_inspection| %>
              <ul class="list-unstyled mb-0">
                <li class="result-text small">&nbsp;&nbsp;･<%= cl_female_inspection.name %></li>
              </ul>
            <% end %>
          <% else %>
            <span class="text-muted small">&nbsp;&nbsp;未検査&nbsp;<span class="small">(未回答)</span></span>
          <% end %><br>
          <% if @report.cl_female_inspection_memo.present? %>
            <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.cl_female_inspection_memo.split("\n"),tag(:br)))).html_safe %></span><br><br>
          <% end %><br>
          <span class="small text-muted"><i class="fas fa-male fa-2x male-icon"></i></span><br>
          <% if @report.cl_male_inspections.present? %>
            <% @report.cl_male_inspections.each do |cl_male_inspection| %>
              <ul class="list-unstyled mb-0">
                <li class="result-text small">&nbsp;&nbsp;･<%= cl_male_inspection.name %></li>
              </ul>
            <% end %>
          <% else %>
            <span class="text-muted small">&nbsp;&nbsp;未検査&nbsp;<span class="small">(未回答)</span></span>
          <% end %><br>
          <% if @report.cl_male_inspection_memo.present? %>
            <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(@report.cl_male_inspection_memo.split("\n"),tag(:br)))).html_safe %></span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center text-center">基本検査<br><small class="small text-muted">【<%= @report.clinic.name %><span class="font-weight-bold">&nbsp;<u>以外で受けた</u>&nbsp;</span>これまでの基本検査】</small></td>
        <td class="pl-3">
          <span class="small text-muted"><i class="fas fa-female fa-2x female-icon"></i></span><br>
          <% if @report.inspections.present? %>
            <% @report.inspections.each do |inspection| %>
              <ul class="list-unstyled mb-0">
                <li class="result-text small">&nbsp;&nbsp;･<%= inspection.name %></li>
              </ul>
            <% end %>
          <% else %>
            <span class="text-muted small">&nbsp;&nbsp;未検査&nbsp;<span class="small">(未回答)</span></span>
          <% end %><br><br>
          <span class="small text-muted"><i class="fas fa-male fa-2x male-icon"></i></span><br>
          <% if @report.male_inspections.present? %>
            <% @report.male_inspections.each do |male_inspection| %>
              <ul class="list-unstyled mb-0">
                <li class="result-text small">&nbsp;&nbsp;･<%= male_inspection.name %></li>
              </ul>
            <% end %>
          <% else %>
            <span class="text-muted small">&nbsp;&nbsp;未検査&nbsp;<span class="small">(未回答)</span></span>
          <% end %>
        </td>
      </tr>
      <% if @report.special_inspections.present? %>
        <% if @special_inspection_era.present? %>
          <tr>
            <td class="result-left-column small text-center ichirantable-left">エラ<br><span class="small text-muted">(ERA)</span><br><span class="small text-muted">【特殊検査】</span></td>
            <td class="pl-1 pr-2 py-1 ichirantable-right">
              <% @special_inspection_era.each do |era| %>
                <div class="mb-0 mt-1 p-0 box5">
                  <table class="table table-hover p-0 mb-0">
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">どこで</td>
                      <% if era.place == 1 %>
                        <td class="pl-3 small" style="width:80%"><%= @report.clinic.name %></td>
                      <% elsif era.place == 2 %>
                        <td class="pl-3 small" style="width:80%">別のクリニックで</td>
                      <% else %>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">費用</td>
                      <% if era.cost.present? %>
                        <td class="pl-3 small" style="width:80%">約&nbsp;<%= era.cost.to_s(:delimited) %>円</td>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">メモ</td>
                      <% if era.memo.present? %>
                        <td class="pl-3 small" style="width:80%">
                          <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(era.memo.split("\n"),tag(:br)))).html_safe %></span>
                        </td>
                      <% end %>
                    </tr>
                  </table>
                </div>
              <% end %>
            </td>
          </tr>
        <% end %>
        <% if @special_inspection_emma.present? %>
          <tr>
            <td class="result-left-column small text-center ichirantable-left">エマ<br><span class="small text-muted">(EMMA)</span><br><span class="small text-muted">【特殊検査】</span></td>
            <td class="pl-1 pr-2 py-1 ichirantable-right">
              <% @special_inspection_emma.each do |emma| %>
                <div class="mb-0 mt-1 p-0 box5">
                  <table class="table table-hover p-0 mb-0">
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">どこで</td>
                      <% if emma.place == 1 %>
                        <td class="pl-3 small" style="width:80%"><%= @report.clinic.name %></td>
                      <% elsif emma.place == 2 %>
                        <td class="pl-3 small" style="width:80%">別のクリニックで</td>
                      <% else %>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">費用</td>
                      <% if emma.cost.present? %>
                        <td class="pl-3 small" style="width:80%">約&nbsp;<%= emma.cost.to_s(:delimited) %>円<br><span class="small text-muted">【特殊検査】</span></td>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">メモ</td>
                      <% if emma.memo.present? %>
                        <td class="pl-3 small" style="width:80%">
                          <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(emma.memo.split("\n"),tag(:br)))).html_safe %></span>
                        </td>
                      <% end %>
                    </tr>
                  </table>
                </div>
              <% end %>
            </td>
          </tr>
        <% end %>
        <% if @special_inspection_alice.present? %>
          <tr>
            <td class="result-left-column small text-center ichirantable-left">アリス<br><span class="small text-muted">(ALICE)</span><br><span class="small text-muted">【特殊検査】</span></td>
            <td class="pl-1 pr-2 py-1 ichirantable-right">
              <% @special_inspection_alice.each do |alice| %>
                <div class="mb-0 mt-1 p-0 box5">
                  <table class="table table-hover p-0 mb-0">
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">どこで</td>
                      <% if alice.place == 1 %>
                        <td class="pl-3 small" style="width:80%"><%= @report.clinic.name %></td>
                      <% elsif alice.place == 2 %>
                        <td class="pl-3 small" style="width:80%">別のクリニックで</td>
                      <% else %>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">費用</td>
                      <% if alice.cost.present? %>
                        <td class="pl-3 small" style="width:80%">約&nbsp;<%= alice.cost.to_s(:delimited) %>円<br><span class="small text-muted">【特殊検査】</span></td>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">メモ</td>
                      <% if alice.memo.present? %>
                        <td class="pl-3 small" style="width:80%">
                          <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(alice.memo.split("\n"),tag(:br)))).html_safe %></span>
                        </td>
                      <% end %>
                    </tr>
                  </table>
                </div>
              <% end %>
            </td>
          </tr>
        <% end %>
        <% if @special_inspection_trio.present? %>
          <tr>
            <td class="result-left-column small text-center ichirantable-left">トリオ<br><span class="small text-muted">(TRIO)</span><br><span class="small text-muted">【特殊検査】</span></td>
            <td class="pl-1 pr-2 py-1 ichirantable-right">
              <% @special_inspection_trio.each do |trio| %>
                <div class="mb-0 mt-1 p-0 box5">
                  <table class="table table-hover p-0 mb-0">
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">どこで</td>
                      <% if trio.place == 1 %>
                        <td class="pl-3 small" style="width:80%"><%= @report.clinic.name %></td>
                      <% elsif trio.place == 2 %>
                        <td class="pl-3 small" style="width:80%">別のクリニックで</td>
                      <% else %>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">費用</td>
                      <% if trio.cost.present? %>
                        <td class="pl-3 small" style="width:80%">約&nbsp;<%= trio.cost.to_s(:delimited) %>円</td>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">メモ</td>
                      <% if trio.memo.present? %>
                        <td class="pl-3 small" style="width:80%">
                          <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(trio.memo.split("\n"),tag(:br)))).html_safe %></span>
                        </td>
                      <% end %>
                    </tr>
                  </table>
                </div>
              <% end %>
            </td>
          </tr>
        <% end %>
        <% if @special_inspection_erpeak.present? %>
          <tr>
            <td class="result-left-column small text-center ichirantable-left">エラピーク<br><span class="small text-muted">(ERPeak)</span><br><span class="small text-muted">【特殊検査】</span></td>
            <td class="pl-1 pr-2 py-1 ichirantable-right">
              <% @special_inspection_erpeak.each do |erpeak| %>
                <div class="mb-0 mt-1 p-0 box5">
                  <table class="table table-hover p-0 mb-0">
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">どこで</td>
                      <% if erapeak.place == 1 %>
                        <td class="pl-3 small" style="width:80%"><%= @report.clinic.name %></td>
                      <% elsif erapeak.place == 2 %>
                        <td class="pl-3 small" style="width:80%">別のクリニックで</td>
                      <% else %>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">費用</td>
                      <% if erpeak.cost.present? %>
                        <td class="pl-3 small" style="width:80%">約&nbsp;<%= erpeak.cost.to_s(:delimited) %>円</td>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">メモ</td>
                      <% if erpeak.memo.present? %>
                        <td class="pl-3 small" style="width:80%">
                          <span class="result-text small"><i data-feather="edit-3" class="feather-icon-style4 pl-0 pt-1 pb-2 mx-0"></i>&nbsp;&nbsp;<%= text_url_to_link(h(safe_join(erpeak.memo.split("\n"),tag(:br)))).html_safe %></span>
                        </td>
                      <% end %>
                    </tr>
                  </table>
                </div>
              <% end %>
            </td>
          </tr>
        <% end %>
        <% if @special_inspection_endometrial_biopsy.present? %>
          <tr>
            <td class="result-left-column small text-center ichirantable-left">子宮内膜組織診<br><span class="small text-muted">【特殊検査】</span></td>
            <td class="pl-1 pr-2 py-1 ichirantable-right">
              <% @special_inspection_endometrial_biopsy.each do |endometrial_biopsy| %>
                <div class="mb-0 mt-1 p-0 box5">
                  <table class="table table-hover p-0 mb-0">
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">どこで</td>
                      <% if endometrial_biopsy.place == 1 %>
                        <td class="pl-3 small" style="width:80%"><%= @report.clinic.name %></td>
                      <% elsif endometrial_biopsy.place == 2 %>
                        <td class="pl-3 small" style="width:80%">別のクリニックで</td>
                      <% else %>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">費用</td>
                      <% if endometrial_biopsy.cost.present? %>
                        <td class="pl-3 small" style="width:80%">約&nbsp;<%= endometrial_biopsy.cost.to_s(:delimited) %>円</td>
                      <% end %>
                    </tr>
                    <tr>
                      <td class="result-left-column small text-center" style="width:20%">メモ</td>
