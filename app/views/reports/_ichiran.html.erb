<div class="cp_box mt-0">
  <input id="cp01" type="checkbox">
  <label for="cp01"></label>
  <div class="cp_container">
    <div class="text-center matome-title mb-3 pt-3">
      <%= link_to user_path(@report.user_id) do %>
        <%= strip_tags(@report.user.name.to_s).gsub(/[\n]/,"").strip.truncate(12) %>&nbsp;<span class="text-muted small">さんの</span>&nbsp;
      <% end %>
      <%= link_to @report.clinic.name, "/category/clinics/#{@report.clinic.id}" %><br>
      <span class="text-muted small">での</span>&nbsp;<span class="font-weight-bold">治療情報 一括縦表示</span>
    </div>

    <table class="table p-0 mt-0">
      <tr>
        <td class="result-left-column" colspan="2">
          <div class="m-0 p-0 form-inline justify-content-center">
            <p class="m-0 p-0 text-muted small">治療</p>&nbsp;&nbsp;<i data-feather="chevrons-right" class="feather-icon-style1"></i>&nbsp;&nbsp;<h6 class="m-0 p-0 font-weight-bold">概要</h6>
          </div>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center ichirantable-left">ステータス</td>
        <td class="pl-3 ichirantable-right">
          <% if @report.current_state.present? %>
            <span class="result-text"><%= @report.str_current_state %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">何人目治療</td>
        <td class="pl-3">
          <% if @report.fertility_treatment_number.present? %>
            <span class="result-text"><%= @report.str_fertility_treatment_number %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center">通院終了</td>
        <td class="pl-3">
          <% if @report.treatment_end_age.present? %>
            <span class="result-text"><%= @report.str_treatment_end_age %></span>
          <% else %>
            <span class="text-muted small">未回答</span>
          <% end %>
          <% if @report.year_of_treatment_end.present? %>
            <span class="result-text small">(<%= @report.year_of_treatment_end.year %>年卒業)</span><br>
          <% end %>
          <% if @report.age_of_partner_at_end_of_treatment.present? %>
            <span class="result-text">パートナー:&nbsp;<%= @report.str_age_of_partner_at_end_of_treatment %></span><br>
          <% end %>

          <% if @report.user.first_age_to_start.present? %>
            <div>
              <small>
                <span class="small text-muted">（妊活は&nbsp;</span><%= @report.user.str_first_age_to_start(@report.user.first_age_to_start) %><span class="small text-muted">&nbsp;から開始）</span>
              </small>
            </div>
          <% else %>
            <div>
              <small>
                <% if user_signed_in? && @report.user_id == current_user.id %>
                  <span class="small text-muted">（妊活は&nbsp;○○歳から開始）</span>
                  <span class="small"><%= link_to "→ 回答する", edit_user_registration_path , { class: "small", style: "color:#009688;"} %></span>
                <% end %>
              </small>
            </div>
          <% end %>
          <% if @report.user.first_age_to_start_art.present? %>
            <div>
              <small>
                <span class="small text-muted">（高度治療は&nbsp;</span><%= @report.user.str_first_age_to_start_art(@report.user.first_age_to_start_art) %><span class="small text-muted">&nbsp;から開始）</span>
              </small>
            </div>
          <% else %>
            <div>
              <small>
                <% if user_signed_in? && @report.user_id == current_user.id %>
                  <span class="small text-muted">（高度治療は&nbsp;○○歳から開始）</span>
                  <span class="small"><%= link_to "→ 回答する", edit_user_registration_path , { class: "small", style: "color:#009688;"} %></span>
                <% end %>
              </small>
            </div>
          <% end %>
        </td>
      </tr>
      <% if @report.treatment_period.present? %>
        <tr>
          <td class="result-left-column small text-center">通院期間</td>
          <td class="pl-3">
            <span class="result-text"><%= @report.str_treatment_period %></span>
          </td>
        </tr>
      <% end %>
      <tr>
        <td class="result-left-column small text-center">受精方法</td>
