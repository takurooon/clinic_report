                </tr>
                <tr>
                  <td class="result-left-column small text-center ichirantable-left">価格</td>
                  <td class="pl-3 ichirantable-right">
                    <% if @report.impression_of_price.present? %>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% if @report.impression_of_price == 1 %>
                      <span class="rating-star small">★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.impression_of_price == 2 %>
                      <span class="rating-star small">★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.impression_of_price == 3 %>
                      <span class="rating-star small">★★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.impression_of_price == 4%>
                      <span class="rating-star small">★★★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<%else @report.impression_of_price == 5 %>
                      <span class="rating-star small">★★★★★</span><% end %>
                    <% else %>
                      &nbsp;&nbsp;&nbsp;&nbsp;<span class="small text-muted">未回答</span>
                    <% end %>
                  </td>
                </tr>
                <tr>
                  <td class="result-left-column small text-center ichirantable-left">待ち時間</td>
                  <td class="pl-3 ichirantable-right">
                    <% if @report.average_waiting_time2.present? %>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% if @report.average_waiting_time2 == 1 %>
                      <span class="rating-star small">★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.average_waiting_time2 == 2 %>
                      <span class="rating-star small">★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.average_waiting_time2 == 3 %>
                      <span class="rating-star small">★★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.average_waiting_time2 == 4%>
                      <span class="rating-star small">★★★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<%else @report.average_waiting_time2 == 5 %>
                      <span class="rating-star small">★★★★★</span><% end %>
                      <% if @report.str_average_waiting_time %>
                        <small><span class="small text-muted">(&nbsp;<%= @report.str_average_waiting_time %>&nbsp;)</span></small>
                      <% end %>
                    <% else %>
                      &nbsp;&nbsp;&nbsp;&nbsp;<span class="small text-muted">未回答</span>
                    <% end %>
                  </td>
                </tr>
                <tr>
                  <td class="result-left-column small text-center ichirantable-left">施設の環境</td>
                  <td class="pl-3 ichirantable-right">
                    <% if @report.comfort_of_space.present? %>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% if @report.comfort_of_space == 1 %>
                      <span class="rating-star small">★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.comfort_of_space == 2 %>
                      <span class="rating-star small">★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.comfort_of_space == 3 %>
                      <span class="rating-star small">★★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% elsif @report.comfort_of_space == 4%>
                      <span class="rating-star small">★★★★</span>
                      &nbsp;&nbsp;&nbsp;&nbsp;<% else @report.comfort_of_space == 5 %>
                      <span class="rating-star small">★★★★★</span><% end %>
                    <% else %>
                      &nbsp;&nbsp;&nbsp;&nbsp;<span class="small text-muted">未回答</span>
                    <% end %>
                  </td>
                </tr>
                <% if (@report.free_wifi.present? && @report.free_wifi <= 3) || (@report.possible_to_wait_outside_cl.present? && @report.possible_to_wait_outside_cl <= 3) || @report.credit_card_validity.present? || (@report.credit_card_validity == 3 && @report.creditcards_can_be_used_from_more_than.present?) %>
                  <tr>
                    <td class="result-left-column small text-center ichirantable-left">備考</td>
                    <td class="pl-3 ichirantable-right">
                      <% if @report.free_wifi.present? && @report.free_wifi <= 3 %>
                        <small>･<span class="small font-weight-bold">&nbsp;<%= @report.str_free_wifi %></span></small><br>
                      <% end %>
                      <% if @report.possible_to_wait_outside_cl.present? && @report.possible_to_wait_outside_cl <= 3 %>
                        <small>･<span class="small font-weight-bold">&nbsp;待ち時間中の<%= @report.str_possible_to_wait_outside_cl %></span></small><br>
                      <% end %>
                      <% if @report.credit_card_validity.present? %>
                        <small>･<span class="small">&nbsp;クレジットカード:&nbsp;<span class="font-weight-bold"><%= @report.str_credit_card_validity %></span></span></small>
                      <% end %>
                      <% if @report.credit_card_validity == 3 %>
                        <% if @report.creditcards_can_be_used_from_more_than.present? %>
                          <small><span class="text-muted small">(<%= @report.creditcards_can_be_used_from_more_than.to_s(:delimited) %>円から利用可能)</span></small>
                        <% end %>
                      <% end %>
                    </td>
                  </tr>
                <% end %>
              </table>
              <% if @report.clinic_review.present? %>
                <div class="p-3">
                  <i data-feather="edit-3" class="feather-icon-style4 pl-0 py-1 mx-0"></i>&nbsp;&nbsp;レビュー<br>
                  <div class="small pt-3"><%= text_url_to_link(h(safe_join(@report.clinic_review.split("\n"),tag(:br)))).html_safe %></div>
                </div>
              <% end %>
              <small class="my-3"><span class="small text-muted">※&nbsp;<%= @report.user.name %> さんの治療当時の情報です。最新情報は直接クリニックに確認して下さい。</span></small>
              <table class="table rounded table-dark p-0 mt-5 mb-2">
                <tr>
                  <td class="small text-center" colspan="2">
                    <% if @report.clinic.current_status == 1 %>
                      <span class="text-danger">【閉院】</span>
                    <% end %>
                    <%= link_to @report.clinic.name, "/clinics/#{@report.clinic.id}", data: { "turbolinks" => false } %>&nbsp;の基本情報
                  </td>
                </tr>
                <tr>
                  <td class="small text-center ichirantable-left">電話</td>
                  <td class="pl-3 ichirantable-right"><i data-feather="phone-outgoing" class="feather-icon-style1 text-primary"></i>&nbsp;&nbsp;&nbsp;<a href="tel:<%= @report.clinic.tel %>"><span class="text-primary"><%= @report.clinic.tel %></span></a></td>
                </tr>
                <tr>
                  <td class="small text-center">住所</td>
                  <td class="pl-3 small">〒<%= @report.clinic.post_code.to_s.insert(3, " - ") %><br><%= @report.clinic.city.prefecture.name %><%= @report.clinic.city.name %><%= @report.clinic.address1 %><br><%= @report.clinic.address2 %></td>
                </tr>
                <tr>
                  <td class="small text-center">備考</td>
                  <td class="pl-3 small">
                    <% if @report.clinic.pgt != 0 %>
                      <%= "・着床前診断(PGT)臨床指定施設" %><br>
                    <% end %>
                    <% if @report.clinic.japco != 0 %>
                      <%= "・PGTコンソーシアム(JAPCO)加盟施設" %><br>
                    <% end %>
                    <% if @report.clinic.jis_art != 0 %>
                      <%= "・JIS-ART登録施設" %><br>
                    <% end %>
                  </td>
                </tr>
              </table>
            </div>
          </div>
        </td>
      </tr>
    </table>

    <table class="table p-0 mt-5 mb-5">
      <tr>
        <td class="result-left-column" colspan="2">
          <div class="m-0 p-0 form-inline justify-content-center">
            <p class="m-0 p-0 text-muted small">分析</p>&nbsp;&nbsp;<i data-feather="chevrons-right" class="feather-icon-style1"></i>&nbsp;&nbsp;<h6 class="m-0 p-0 font-weight-bold">データ分析</h6>
          </div>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center ichirantable-left">受精率<i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
        <td class="pl-3 ichirantable-right">
          <% if @report.number_of_eggs_collected.present? && @report.number_of_eggs_collected <= 50 && @report.number_of_fertilized_eggs.present? && @report.number_of_fertilized_eggs <= 50 %>
            <% juseiritsu = (@report.number_of_fertilized_eggs.to_f / @report.number_of_eggs_collected.to_f) * 100 %>
              <% if juseiritsu <= 100.00 %>
                <span class="result-text"><%= sprintf("%.2f", juseiritsu) %>%<small><br><span class="small text-muted">=&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></small></span>
              <% else %>
                <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></span>
              <% end %>
          <% else %>
            <span class="text-muted small">算出不可</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center" style="width:30%">移植可能胚到達率<span class="small text-muted">(対 採卵)</span><i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
        <td class="pl-3" style="width:70%">
          <% if @report.number_of_eggs_collected.present? && @report.number_of_eggs_collected <= 50 && @report.number_of_transferable_embryos.present? && @report.number_of_transferable_embryos <= 50 %>
            <% ishokukanouhai_sairan = (@report.number_of_transferable_embryos.to_f / @report.number_of_eggs_collected.to_f) * 100 %>
            <% if ishokukanouhai_sairan <= 100.00 %>
              <span class="result-text"><%= sprintf("%.2f", ishokukanouhai_sairan) %>%<small><br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></small></span>
            <% else %>
              <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></span>
            <% end %>
          <% else %>
            <span class="text-muted small">算出不可</span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td class="result-left-column small text-center" style="width:30%">移植可能胚到達率<span class="small text-muted">(対 受精卵)</span><i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
        <td class="pl-3" style="width:70%">
          <% if @report.number_of_fertilized_eggs.present? && @report.number_of_fertilized_eggs <= 50 && @report.number_of_transferable_embryos.present? && @report.number_of_transferable_embryos <= 50 %>
            <% ishokukanouhai_juseiran = (@report.number_of_transferable_embryos.to_f / @report.number_of_fertilized_eggs.to_f) * 100 %>
            <% if ishokukanouhai_juseiran <= 100.00 %>
              <span class="result-text"><%= sprintf("%.2f", ishokukanouhai_juseiran) %>%<small><br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %></span></small></span>
            <% else %>
              <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %></span></span>
            <% end %>
          <% else %>
            <span class="text-muted small">算出不可</span>
          <% end %>
        </td>
      </tr>
      <% if @report.current_state.present? && @report.current_state < 5 %>
        <tr>
          <td class="result-left-column small text-center" style="width:30%">採卵あたりの妊娠率<i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
          <td class="pl-3" style="width:70%">
            <% if @report.current_state == 1 || @report.current_state == 3 %>
              <% ninshinritsu_sairan = ( 1 / @report.number_of_eggs_collected.to_f ) * 100 %>
              <% if ninshinritsu_sairan <= 100.00 %>
                <span class="result-text"><%= sprintf("%.2f", ninshinritsu_sairan) %>%<small><br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;</span></small></span>
              <% else %>
                <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;</span></span>
              <% end %>
            <% else %>
              <span class="text-muted small">算出不可</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small text-center" style="width:30%">移植あたりの妊娠率<span class="small text-muted">(回数)</span><i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
          <td class="pl-3" style="width:70%">
            <% if @report.total_number_of_transplants.present? && @report.total_number_of_transplants <= 50 %>
              <% ninshinritsu_ishoku_kaisu = ( 1 / @report.total_number_of_transplants.to_f) * 100 %>
              <% if ninshinritsu_ishoku_kaisu < 100.00 %>
                <span class="result-text"><%= sprintf("%.2f", ninshinritsu_ishoku_kaisu) %>%<small><br><span class="small text-muted">移植回数&nbsp;<%= @report.str_total_number_of_transplants %></span></small></span>
              <% else %>
                <span class="text-muted small">算出不可<br><span class="small text-muted">移植回数&nbsp;<%= @report.str_total_number_of_transplants %></span></span>
              <% end %>
            <% else %>
              <span class="text-muted small">算出不可</span>
            <% end %>
          </td>
        </tr>
        <tr>
          <td class="result-left-column small text-center" style="width:30%">移植あたりの妊娠率<span class="small text-muted">(個数)</span><i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
          <td class="pl-3" style="width:70%">
            <% if @report.total_number_of_eggs_transplanted.present? && @report.total_number_of_eggs_transplanted <= 50 %>
              <% if @report.current_state == 2 || @report.current_state == 4 %>
                <% ninshinritsu_ishoku_kaisu = ( 2 / @report.total_number_of_eggs_transplanted.to_f) * 100 %>
                <% if ninshinritsu_ishoku_kaisu < 100.00 %>
                  <span class="result-text"><%= sprintf("%.2f", ninshinritsu_ishoku_kaisu) %>%<small><br><span class="small text-muted">=&nbsp;2個&nbsp;/&nbsp;移植個数&nbsp;<%= @report.str_total_number_of_eggs_transplanted %>(<%= @report.str_total_number_of_transplants %>)</span></small></span>
                <% else %>
                  <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;2個&nbsp;/&nbsp;移植個数&nbsp;<%= @report.str_total_number_of_eggs_transplanted %>(<%= @report.str_total_number_of_transplants %>)</span></span>
                <% end %>
              <% elsif @report.current_state == 1 || @report.current_state == 3 %>
                <% ninshinritsu_ishoku_kaisu = ( 1 / @report.total_number_of_eggs_transplanted.to_f) * 100 %>
                <% if ninshinritsu_ishoku_kaisu <= 100.00 %>
                  <span class="result-text"><%= sprintf("%.2f", ninshinritsu_ishoku_kaisu) %>%<small><br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;移植個数&nbsp;<%= @report.str_total_number_of_eggs_transplanted %>(<%= @report.str_total_number_of_transplants %>)</span></small></span>
                <% else %>
                  <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;移植個数&nbsp;<%= @report.str_total_number_of_eggs_transplanted %>(<%= @report.str_total_number_of_transplants %>)</span></span>
                <% end %>
              <% else %>
              <% end %>
            <% else %>
              <span class="text-muted small">算出不可</span>
            <% end %>
          </td>
        </tr>
      <% end %>
    </table>

  </div>
</div>
