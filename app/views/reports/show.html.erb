<div class="container main-contents-show px-0 m-bottom">
  <div class="d-none d-lg-block position-fixed mt-5 pt-5">
    <div class="text-center mt-5 mx-0 mb-0 p-0">
      <p style="font-size: 8px;" class="text-muted mb-1">＼URLをコピー／</p>
    </div>
    <div class="text-center mt-2 mb-3 pb-1">
      <button type="button" class="btn bmd-btn-fab" id="copy-page" title="URLをコピー！" style="background-color: #2ECFC0;">
        <i data-feather="copy" style="color: #ffffff;"></i>
      </button>
    </div>

    <div class="text-center mt-5 p-0">
      <p style="font-size: 8px;" class="text-muted mb-1">＼用語／</p>
    </div>
    <div class="text-center my-2"><%= link_to image_tag("action/book.svg", alt: "terminology", width: "55px", class: "pl-1", loading: "lazy"), terminology_path, title: 'Terminology', "data-toggle": "modal", "data-target": "#terminology" %></div>

    <div class="text-center mt-5 p-0">
      <p style="font-size: 8px;" class="text-muted mb-1">＼シェア／</p>
    </div>

    <% report_title = strip_tags(@report.title.to_s).gsub(/[\n]/,"").strip.truncate(26) %>
    <% report_title_long = strip_tags(@report.title.to_s).gsub(/[\n]/,"").strip.truncate(50) %>
    <% user_name = strip_tags(@report.user.name.to_s).gsub(/[\n]/,"").strip.truncate(12) %>
    <% if @report.treatment_end_age.present? %>
      <% treatment_end_age = "(#{@report.str_treatment_end_age})" %>
      <% treatment_end_age2 = "#{@report.str_treatment_end_age}: " %>
    <% else %>
      <% treatment_end_age = "" %>
      <% treatment_end_age2 = "" %>
    <% end %>
    <% if @report.year_of_treatment_end.present? %>
      <% year_of_treatment_end = "(#{@report.year_of_treatment_end.year.to_s}年卒業)" %>
      <% year_of_treatment_end2 = "･･･#{@report.year_of_treatment_end.year.to_s}年卒業" %>
    <% else %>
      <% year_of_treatment_end = "" %>
      <% year_of_treatment_end2 = "" %>
    <% end %>
    <% author = " #{user_name}さんの" %>
    <% if @report.title.present? %>
      <% title = "#{report_title} | REPOCO" %>
      <% sns_text = "【#{treatment_end_age2}#{@report.clinic.name} (#{@report.clinic.prefecture.name})での#{author}レポコ#{year_of_treatment_end2}】 " + "#{report_title_long}" %>
    <% else %>
      <% title = "#{@report.clinic.name}のレポコ | REPOCO" %>
      <% sns_text = "【#{treatment_end_age2}#{@report.clinic.name} (#{@report.clinic.prefecture.name})での#{author}レポコ#{year_of_treatment_end2}】 " %>
    <% end %>

    <div class="text-center my-2">
      <%= link_to image_tag("icon/twitter_side_brandcolor_icon.png", alt: "twitter_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "https://twitter.com/intent/tweet?text=#{sns_text}&hashtags=レポコ,REPOCO&url=#{request.url}", class: 'twitter', title: 'Twitter', target: '_blank' %>
    </div>
  </div>
  <div class="row">
    <% set_meta_tags og: {image: "https://res.cloudinary.com/dt8ullid9/image/upload/l_text:Sawarabi%20Gothic_22:【#{@report.clinic.name} #{year_of_treatment_end}】#{@report.user.name}さん#{treatment_end_age}のレポコ,co_rgb:333,w_400,c_fit/v1579444928/ogp_476_298_brandcolor3_3_ukjdnb.png"} %>
    <% set_meta_tags twitter: {title: "#{title}", image: "https://res.cloudinary.com/dt8ullid9/image/upload/l_text:Sawarabi%20Gothic_22:【#{@report.clinic.name} #{year_of_treatment_end}】#{@report.user.name}さん#{treatment_end_age}のレポコ,co_rgb:333,w_400,c_fit/v1579444928/ogp_476_298_brandcolor3_3_ukjdnb.png"} %>

    <div class="col-sm-12 offset-md-0 offset-lg-1 col-lg-8 main-column">
      <div class="d-none d-sm-block">
        <div class="row pt-3 pb-0">
          <div class="col-2 mt-2 d-flex align-items-center justify-content-center">
            <div class="py-0 pl-0">
              <%= link_to user_path(@report.user_id), data: { "turbolinks" => false }, style: "text-decoration: none;" do %>
                <% if @report.user.icon.attached? %>
                  <%= image_tag(@report.user.icon, alt: "icon", class: "icon_image2", loading: "lazy") %>
                <% else %>
                  <% if @report.treatment_end_age == nil %>
                    <%= image_tag("icon/g_blank.png", alt: "generation_blank", class: "icon_image2_border", loading: "lazy", title: "不明") %>
                  <% elsif @report.treatment_end_age < 25 %>
                    <%= image_tag("icon/g_10_24.png", alt: "generation10_24", class: "icon_image2_border", loading: "lazy", title: "〜24歳") %>
                  <% elsif @report.treatment_end_age < 30 %>
                    <%= image_tag("icon/g_25_29.png", alt: "generation25_29", class: "icon_image2_border", loading: "lazy", title: "25〜29歳") %>
                  <% elsif @report.treatment_end_age < 35 %>
                    <%= image_tag("icon/g_30_34.png", alt: "generation30_34", class: "icon_image2_border", loading: "lazy", title: "30〜34歳") %>
                  <% elsif @report.treatment_end_age < 40 %>
                    <%= image_tag("icon/g_35_39.png", alt: "generation35_39", class: "icon_image2_border", loading: "lazy", title: "35〜39歳") %>
                  <% elsif @report.treatment_end_age < 45 %>
                    <%= image_tag("icon/g_40_44.png", alt: "generation40_44", class: "icon_image2_border", loading: "lazy", title: "40〜44歳") %>
                  <% else %>
                    <%= image_tag("icon/g_45_.png", alt: "generation45_", class: "icon_image2_border", loading: "lazy", title: "45歳〜") %>
                  <% end %>
                <% end %><br>
                <div class="m-0 pt-1 small text-center text-dark">
                  <%= strip_tags(@report.user.name.to_s).gsub(/[\n]/,"").strip.truncate(10) %>
                </div>
              <% end %>
            </div>
          </div>
          <div class="col-10 mt-2">
            <div class="row">
              <% if @report.user == current_user %>
                <div class="col-12 text-right">
                  <%= link_to '削除', report_path(@report), method: :delete, data: { confirm: '本当に削除してもよろしいですか？'}, class:"btn btn-warning float-right" %>
                  <%= link_to '編集', edit_report_path(@report), class:"btn btn-secondary float-right mx-1" %>
                  <% if @report.released? %>
                    <%= link_to '非公開', nonrelease_report_path(@report), method: :patch, data: { confirm: '非公開にして良いですか？'}, class:"btn btn-secondary float-right" %>
                  <% else %>
                    <%= link_to '公開', release_report_path(@report), method: :patch, data: { confirm: '公開して良いですか？'}, class:"btn btn-secondary btn-info float-right" %>
                  <% end %>
                </div>
                <div class="col-12 text-right">
                  <%= link_to edit_user_registration_path do %>
                    <div class="pt-0 pr-3 small">
                      <i data-feather="edit-3" class="feather-icon-style1 pb-1"></i>プロフィールを編集
                    </div>
                  <% end %>
                </div>
              <% else %>
                <div class="col-12">
                  <div class="row">
                    <% if @report.user.self_introduction.present? && @report.comments.present? %>
                      <div class="col-6 d-flex align-items-center">
                        <div class="balloon1-left small d-flex align-items-center">
                          <%= link_to user_path(@report.user_id), data: { "turbolinks" => false } do %>
                            <span class="text-muted"><%= strip_tags(@report.user.self_introduction.to_s).gsub(/[\n]/,"").strip.truncate(50) %></span>
                          <% end %>
                        </div>
                      </div>
                      <div class="col-6">
                        <div class="alert rounded-lg pt-2 pb-1 px-3 mt-3" style="background-color: #F7F9F9">
                          <a href="#comments_area">
                            <p class="mb-0 small text-muted">#コメント(<%= @report.comments.count %>件)<span class="small">&nbsp;&nbsp;&nbsp;&nbsp;<i class="far fa-hand-point-up fa-lg"></i></span></p>
                            <p class="my-2 small text-dark"><i data-feather="message-circle" class="pb-1"></i>&nbsp;&nbsp<span class="small"><%= strip_tags(@report.comments.first.comment.to_s).gsub(/[\n]/,"").strip.truncate(40) %></span>&nbsp;&nbsp;<span class="small text-muted">(<%= strip_tags(@report.comments.first.user.name.to_s).gsub(/[\n]/,"").strip.truncate(8) %>さん)</span></p>
                          </a>
                        </div>
                      </div>
                    <% elsif @report.user.self_introduction.present? && @report.comments.blank? %>
                      <div class="col-12 d-flex align-items-center">
                        <div class="balloon1-left small d-flex align-items-center">
                          <%= link_to user_path(@report.user_id), data: { "turbolinks" => false } do %>
                            <span class="text-muted"><%= strip_tags(@report.user.self_introduction.to_s).gsub(/[\n]/,"").strip.truncate(100) %></span>
                          <% end %>
                        </div>
                      </div>
                    <% elsif @report.user.self_introduction.blank? && @report.comments.present? %>
                      <div class="col-12">
                        <div class="alert rounded-lg pt-2 pb-1 px-3 mt-3" style="background-color: #F7F9F9">
                          <a href="#comments_area">
                            <p class="mb-0 small text-muted">#コメント(<%= @report.comments.count %>件)<span class="small">&nbsp;&nbsp;&nbsp;&nbsp;<i class="far fa-hand-point-up fa-lg"></i></span></p>
                            <p class="my-2 small text-dark"><i data-feather="message-circle" class="pb-1"></i>&nbsp;&nbsp;<span class="small"><%= strip_tags(@report.comments.first.comment.to_s).gsub(/[\n]/,"").strip.truncate(100) %></span>&nbsp;&nbsp;<span class="small text-muted">(<%= strip_tags(@report.comments.first.user.name.to_s).gsub(/[\n]/,"").strip.truncate(8) %>さん)</span></p>
                          </a>
                        </div>
                      </div>
                    <% else %>
                    <% end %>
                  </div>
                </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>

      <div class="d-block d-sm-none">
        <div class="row pt-2 pb-0">
          <div class="col-3 pl-4 pr-2 pt-3 pb-2 d-flex align-items-center justify-content-center">
            <%= link_to user_path(@report.user_id), data: { "turbolinks" => false }, style: "text-decoration: none;" do %>
              <% if @report.user.icon.attached? %>
                  <%= image_tag(@report.user.icon, alt: "icon", class: "icon_image3", loading: "lazy") %>
              <% else %>
                <% if @report.treatment_end_age == nil %>
                  <%= image_tag("icon/g_blank.png", alt: "generation_blank", class: "icon_image3_border", loading: "lazy", title: "不明") %>
                <% elsif @report.treatment_end_age < 25 %>
                  <%= image_tag("icon/g_10_24.png", alt: "generation10_24", class: "icon_image3_border", loading: "lazy", title: "〜24歳") %>
                <% elsif @report.treatment_end_age < 30 %>
                  <%= image_tag("icon/g_25_29.png", alt: "generation25_29", class: "icon_image3_border", loading: "lazy", title: "25〜29歳") %>
                <% elsif @report.treatment_end_age < 35 %>
                  <%= image_tag("icon/g_30_34.png", alt: "generation30_34", class: "icon_image3_border", loading: "lazy", title: "30〜34歳") %>
                <% elsif @report.treatment_end_age < 40 %>
                  <%= image_tag("icon/g_35_39.png", alt: "generation35_39", class: "icon_image3_border", loading: "lazy", title: "35〜39歳") %>
                <% elsif @report.treatment_end_age < 45 %>
                  <%= image_tag("icon/g_40_44.png", alt: "generation40_44", class: "icon_image3_border", loading: "lazy", title: "40〜44歳") %>
                <% else %>
                  <%= image_tag("icon/g_45_.png", alt: "generation45_", class: "icon_image3_border", loading: "lazy", title: "45歳〜") %>
                <% end %>
              <% end %><br>
              <div class="m-0 pt-0 small text-center text-dark">
                <span class="small"><%= strip_tags(@report.user.name.to_s).gsub(/[\n]/,"").strip.truncate(8) %></span>
              </div>
            <% end %>
          </div>
          <div class="col-9 pl-0 d-flex align-items-center">
            <div class="container-fluid">
              <div class="row">
                <% if @report.user == current_user %>
                  <div class="col-12 text-right pr-0">
                    <%= link_to '削除', report_path(@report), method: :delete, data: { confirm: '本当に削除してもよろしいですか？'}, class:"btn btn-warning float-right" %>
                    <%= link_to '編集', edit_report_path(@report), class:"btn btn-secondary float-right mx-1" %>
                    <% if @report.released? %>
                      <%= link_to '非公開', nonrelease_report_path(@report), method: :patch, data: { confirm: '非公開にして良いですか？'}, class:"btn btn-secondary float-right" %>
                    <% else %>
                      <%= link_to '公開', release_report_path(@report), method: :patch, data: { confirm: '公開して良いですか？'}, class:"btn btn-secondary btn-info float-right" %>
                    <% end %>
                  </div>
                  <div class="col-12 text-right pr-0">
                    <%= link_to edit_user_registration_path do %>
                      <div class="pt-0 pr-3 small">
                        <i data-feather="edit-3" class="feather-icon-style1 pb-1"></i>プロフィールを編集
                      </div>
                    <% end %>
                  </div>
                <% else %>
                  <% if @report.comments.present? %>
                    <div class="col-12 px-0">
                      <div class="alert rounded-lg ml-0 mr-2 mb-0 p-0" style="background-color: #F7F9F9">
                        <div class="pt-3 pb-0 mr-0">
                          <div class="small d-flex align-items-center px-3 pb-2 mb-0">
                            <a href="#comments_area">
                              <p class="mb-0 small text-muted">#コメント(<%= @report.comments.count %>件)<span class="small">&nbsp;&nbsp;&nbsp;&nbsp;<i class="far fa-hand-point-up fa-lg"></i></span></p>
                              <p class="my-2 small text-dark"><i data-feather="message-circle" class="pb-1"></i>&nbsp;&nbsp;<%= strip_tags(@report.comments.first.comment.to_s).gsub(/[\n]/,"").strip.truncate(40) %>&nbsp;&nbsp;<span class="small text-muted">(<%= strip_tags(@report.comments.first.user.name.to_s).gsub(/[\n]/,"").strip.truncate(8) %>さん)</span></p>
                            </a> 
                          </div>
                        </div>
                      </div>
                    </div>
                  <% elsif @report.comments.blank? && @report.user.self_introduction.present? %>
                    <div class="col-12 d-flex align-items-center">
                      <div class="balloon1-left small d-flex align-items-center">
                        <%= link_to user_path(@report.user_id), data: { "turbolinks" => false } do %>
                          <span class="text-muted"><%= strip_tags(@report.user.self_introduction.to_s).gsub(/[\n]/,"").strip.truncate(50) %></span>
                        <% end %>
                      </div>
                    </div>
                  <% else %>
                  <% end %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="mt-4 mb-0 pt-2 px-0 d-none d-sm-block">
        <h5 class="d-none d-sm-block mb-3"><%= link_to "##{@report.clinic.name}", "/clinic/#{@report.clinic.id}", class: "text-dark" %>&nbsp;<span class="text-muted" style="font-size:14px">(<%= link_to @report.clinic.city.prefecture.name, "/clinics_/#{@report.clinic.city.prefecture.name_alphabet}" %>&nbsp;<%= link_to @report.clinic.city.name, "/clinics/#{@report.clinic.city.prefecture.name_alphabet}/#{@report.clinic.city.name_alphabet}" %>)&nbsp;</span>のレポコ</h5>
      </div>
      <div class="mt-3 mb-0 px-2 d-block d-sm-none">
        <h6 class="d-block d-sm-none mb-3"><%= link_to "##{@report.clinic.name}", "/clinic/#{@report.clinic.id}", class: "text-dark" %>&nbsp;<span class="text-muted" style="font-size:14px">(<%= link_to @report.clinic.city.prefecture.name, "/clinics_/#{@report.clinic.city.prefecture.name_alphabet}" %>&nbsp;<%= link_to @report.clinic.city.name, "/clinics/#{@report.clinic.city.prefecture.name_alphabet}/#{@report.clinic.city.name_alphabet}" %>)&nbsp;</span>のレポコ</h6>
      </div>

      <div class="mt-3 mb-1 px-2 small">
        <% if @report.treatment_end_age.present? %>
          <%= link_to "/age/#{@report.treatment_end_age}", style: "text-decoration: none;" do %>
            <span class="badge badge-light text-muted">#<%= @report.treatment_end_age %>歳</span>
          <% end %>
        <% end %>
        <% if @report.city.prefecture.present? && @report.prefecture_at_the_time_status == "show" %>
          <%= link_to "/users_/#{@report.city.prefecture.name_alphabet}", style: "text-decoration: none;" do %>
            <span class="badge badge-light text-muted">#<%= @report.city.prefecture.name %></span>
          <% end %>
        <% end %>
        <% if @report.city.present? && @report.city_at_the_time_status == "show" %>
          <%= link_to "/users_/#{@report.city.prefecture.name_alphabet}/#{@report.city.name_alphabet}", style: "text-decoration: none;" do %>
            <span class="badge badge-light text-muted">#<%= @report.city.name %></span>
          <% end %>
        <% end %>
        <div class="text-right">
          <span class="small text-muted">投稿:&nbsp;<%= @report.created_at.to_s(:datetime_jp) %></span>&nbsp;&nbsp;
          <span class="small text-muted">更新:&nbsp;<%= @report.updated_at.to_s(:datetime_jp) %></span>
        </div>
      </div>

      <div class="report-main-content">
        <% if false %>
        <div class="d-none d-md-block col-md-12 mt-4 mb-4">
          <div class='row mb-3 sns-bar'>
            <div class="col-md-2 twitter-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/twitter_bar_brandcolor_icon.png", alt: "twitter_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "https://twitter.com/share?url=#{request.url}&text=タイトル", class: 'twitter', title: 'Twitter', target: '_blank' %></div>
            <div class="col-md-2 facebook-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/facebook_bar_brandcolor_icon.png", alt: "facebook_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "https://www.facebook.com/sharer/sharer.php?u=#{request.url}&text=タイトル", class: 'facebook', title: 'Facebook', target: '_blank' %></div>
            <div class="col-md-2 line-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/line_bar_brandcolor_icon.png", alt: "line_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://line.me/R/msg/text/?#{request.url}&text=タイトル", class: 'line', title: 'LINE', target: '_blank' %></div>
            <div class="col-md-2 note-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/note_bar_brandcolor_icon.png", alt: "note_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "https://note.mu/intent/post?url=#{request.url}&text=タイトル", class: 'note', title: 'Note', target: '_blank' %></div>
            <div class="col-md-2 slack-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/slack_bar_brandcolor_icon.png", alt: "slack_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://slackbutton.herokuapp.com/post/new/?url=#{request.url}&text=タイトル", class: 'slack', title: 'Slack', target: '_blank' %></div>
            <div class="col-md-2 hatena-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/hatena_bar_brandcolor_icon.png", alt: "hatena_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://b.hatena.ne.jp/add?mode=confirm&url=#{request.url}&text=タイトル", class: 'hatena', title: 'Hatena', target: '_blank' %></div>
          </div>
        </div>
        <% end %>

        <% if @report.year_of_treatment_end.present? && Time.now.year - @report.year_of_treatment_end.year > 0 %>
          <div class="alert alert-info small my-3 py-1" role="alert">
            こちらのレポコは&nbsp;<%= @report.year_of_treatment_end.year %>年&nbsp;<span class="small text-muted">(約<%= Time.now.year - @report.year_of_treatment_end.year %>年前)</span>&nbsp;の治療情報です。
          </div>
        <% end %>
        
        <div class="action_area pl-2 py-0 mx-0 my-1">
          <div class="row">
            <div class="col-12 form-inline">
              <div class="likes_buttons_<%= @report.id %>">
                <%= render partial: 'likes/like', locals: { report: @report}, style:"text-decoration: none;"  %>
              </div>
            </div>
            <p class="mb-0 text-muted pl-2 pt-0" style="font-size: 5px;">&nbsp;&nbsp;Stock&nbsp;!</p>
          </div>
        </div>

        <div class="parent-tab landing-point background-color">
          <% if user_signed_in? %>
            <p class="d-xs-block d-md-none float-left mb-0 px-2 d-inline" style="font-size:15px">
              <% if current_user.already_liked?(@report) %>
                <%= images_action_svg("like_mobile.svg") %><span class="text-danger">&nbsp;&nbsp;<% if @report.likes.present? %><%= @report.likes.count %><% end %></span>
              <% else %>
                <%= images_action_svg("unlike_mobile.svg") %><span class="text-muted">&nbsp;&nbsp;<% if @report.likes.present? %><%= @report.likes.count %><% end %></span>
              <% end %>
            </p>
          <% end %>
          <p class="d-xs-block d-md-none float-right text-success mb-0 px-2" style="font-size:8px">※&nbsp;CL&nbsp;=&nbsp;クリニック(Clinic)</p><br>
          <p class="d-none d-md-block float-right text-success" style="font-size:8px">※&nbsp;CL&nbsp;=&nbsp;クリニック(Clinic)</p>
          <ul class="nav nav-tabs parent-menu mt-2">
            <li class="tab-item"><a href="#tab_1" class="active mostleft-tab" data-toggle="tab">治療</a></li>
            <li class="tab-item"><a href="#tab_2" data-toggle="tab">ユーザー</a></li>
            <li class="tab-item"><a href="#tab_3" data-toggle="tab">クリニック</a></li>
            <li class="tab-item"><a href="#tab_4" data-toggle="tab">分析</a></li>
          </ul>
          <section class="parent-tab-content active mostleft-tab" id="tab_1">
            <div class="child-tab">
              <ul class="nav nav-tabs child-menu">
                <li class="tab-item"><a href="#tab_1_1" class="active" data-toggle="tab">概要</a></li>
                <li class="tab-item"><a href="#tab_1_2" data-toggle="tab">検査</a></li>
                <li class="tab-item"><a href="#tab_1_3" data-toggle="tab">採卵</a></li>
                <li class="tab-item"><a href="#tab_1_4" data-toggle="tab">移植</a></li>
