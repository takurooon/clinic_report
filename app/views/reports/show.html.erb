<%= javascript_include_tag 'report_show' %>

<div class="contaner px-0">
  <div class="row">
    <div class="d-none d-lg-block position-fixed mt-5 pt-4">
      <div class="py-2"><%= link_to image_tag("icon/twitter_side_brandcolor_icon.png", alt: "twitter_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "https://twitter.com/share?url=#{request.url}&text=タイトル", class: 'twitter', title: 'Twitter', target: '_blank' %></div>
      <div class="py-2"><%= link_to image_tag("icon/facebook_side_brandcolor_icon.png", alt: "facebook_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "https://www.facebook.com/sharer/sharer.php?u=#{request.url}&text=タイトル", class: 'facebook', title: 'Facebook', target: '_blank' %></div>
      <div class="py-2"><%= link_to image_tag("icon/line_side_brandcolor_icon.png", alt: "line_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "http://line.me/R/msg/text/?#{request.url}&text=タイトル", class: 'line', title: 'LINE', target: '_blank' %></div>
      <div class="py-2"><%= link_to image_tag("icon/slack_side_brandcolor_icon.png", alt: "slack_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "http://slackbutton.herokuapp.com/post/new/?url=#{request.url}&text=タイトル", class: 'slack', title: 'Slack', target: '_blank' %></div>
      <div class="py-2"><%= link_to image_tag("icon/hatena_side_brandcolor_icon.png", alt: "hatena_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "http://b.hatena.ne.jp/add?mode=confirm&url=#{request.url}&text=タイトル", class: 'hatena', title: 'Hatena', target: '_blank' %></div>
      <div class="py-2"><%= link_to image_tag("icon/pocket_side_brandcolor_icon.png", alt: "pocket_side_brandcolor_icon", width: "55px", class: "img-fluid", loading: "lazy"), "http://getpocket.com/edit?url=#{request.url}&text=タイトル", class: 'pocket', title: 'Pocket', target: '_blank' %></div>
    </div>
    <div class="col-sm-12 col-md-12 offset-md-0 offset-lg-1 col-lg-8 main-column">
      <% set_meta_tags og: {image: "https://res.cloudinary.com/dt8ullid9/image/upload/l_text:Helvetica_40:【#{@report.clinic.name}】#{@report.user.name}さん(#{@report.str_treatment_end_age})のレポコ,co_rgb:333,w_650,c_fit/v1579444928/ogp_800_420_brandcolor_uc1cw8.png"} %>

      <% if @report.user == current_user %>
        <div class="py-3">
          <%= link_to '削除', report_path(@report), method: :delete, data: { confirm: '本当に削除してもよろしいですか？'}, class:"btn btn-warning float-right" %>
          <%= link_to '編集', edit_report_path(@report), class:"btn btn-secondary float-right mx-1" %>
          <% if @report.released? %>
            <%= link_to '非公開にする', nonrelease_report_path(@report), method: :patch, data: { confirm: '非公開にして良いですか？'}, class:"btn btn-secondary float-right" %>
          <% else %>
            <%= link_to '公開する', release_report_path(@report), method: :patch, data: { confirm: '公開して良いですか？'}, class:"btn btn-secondary btn-info float-right" %>
          <% end %>
        </div>
      <% end %>

      <div class="d-flex align-items-center mt-5 mb-5 px-2">
        <h4 class="d-none d-sm-block"><%= link_to @report.user.name, user_path(@report.user_id) %>さんの&nbsp;&nbsp;<%= @report.clinic.name %><span class="text-muted" style="font-size:14px">(<%= link_to @report.clinic.city.prefecture.name, "/category/clinics/prefecture/#{@report.clinic.city.prefecture.name}" %>&nbsp;<%= link_to @report.clinic.city.name, "/category/clinics/city/#{@report.clinic.city.name}" %>)</span>でのレポコ</h4>
        <h5 class="d-block d-sm-none"><%= link_to @report.user.name, user_path(@report.user_id) %>さんの&nbsp;&nbsp;<%= @report.clinic.name %><span class="text-muted" style="font-size:14px">(<%= link_to @report.clinic.city.prefecture.name, "/category/clinics/prefecture/#{@report.clinic.city.prefecture.name}" %>&nbsp;<%= link_to @report.clinic.city.name, "/category/clinics/city/#{@report.clinic.city.name}" %>)</span>でのレポコ</h5>
      </div>

      <div class="mb-1 px-2">
        <% if @report.treatment_end_age.present? %>
          <span class="badge badge-light"><%= link_to @report.treatment_end_age, "/category/age/#{@report.treatment_end_age}", class: "text-dark" %>歳</span>
        <% end %>
        <% if @report.city.prefecture.present? %>
          <span class="badge badge-pill badge-primary"><%= link_to @report.city.prefecture.name, "/category/area/prefecture/#{@report.city.prefecture.id}", class: "text-light" %></span>
        <% end %>
        <% if @report.city.present? %>
          <span class="badge badge-pill badge-primary"><%= link_to @report.city.name, "/category/area/city/#{@report.city.id}",  class: "text-light" %></span>
        <% end %>
        <% @report.tags.each do |tag| %>
          <span class="badge badge-info"><%= link_to tag.tag_name, "/category/tags/#{tag.tag_name}", class: "text-light" %></span>
        <% end %>
        <div class="text-right">
          <span class="small text-muted">投稿:&nbsp;<%= @report.created_at.to_s(:datetime_jp) %></span>&nbsp;&nbsp;
          <span class="small text-muted">更新:&nbsp;<%= @report.updated_at.to_s(:datetime_jp) %></span>
        </div>
      </div>

      <div class="report-main-content">
        <div class="d-none d-md-block col-md-12 mt-4 mb-4">
          <div class='row mb-3 sns-bar'>
            <div class="col-md-2 twitter-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/twitter_bar_brandcolor_icon.png", alt: "twitter_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "https://twitter.com/share?url=#{request.url}&text=タイトル", class: 'twitter', title: 'Twitter', target: '_blank' %></div>
            <div class="col-md-2 facebook-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/facebook_bar_brandcolor_icon.png", alt: "facebook_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "https://www.facebook.com/sharer/sharer.php?u=#{request.url}&text=タイトル", class: 'facebook', title: 'Facebook', target: '_blank' %></div>
            <div class="col-md-2 line-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/line_bar_brandcolor_icon.png", alt: "line_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://line.me/R/msg/text/?#{request.url}&text=タイトル", class: 'line', title: 'LINE', target: '_blank' %></div>
            <div class="col-md-2 slack-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/slack_bar_brandcolor_icon.png", alt: "slack_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://slackbutton.herokuapp.com/post/new/?url=#{request.url}&text=タイトル", class: 'slack', title: 'Slack', target: '_blank' %></div>
            <div class="col-md-2 hatena-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/hatena_bar_brandcolor_icon.png", alt: "hatena_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://b.hatena.ne.jp/add?mode=confirm&url=#{request.url}&text=タイトル", class: 'hatena', title: 'Hatena', target: '_blank' %></div>
            <div class="col-md-2 pocket-button d-flex align-items-center justify-content-center p-0"><%= link_to image_tag("icon/pocket_bar_brandcolor_icon.png", alt: "pocket_bar_brandcolor_icon", width: "400px", class: "img-fluid", loading: "lazy"), "http://getpocket.com/edit?url=#{request.url}&text=タイトル", class: 'pocket', title: 'Pocket', target: '_blank' %></div>
          </div>
        </div>

        <% if @report.year_of_treatment_end.present? && Time.now.year - @report.year_of_treatment_end.year > 0 %>
          <div class="alert alert-info small my-3 py-1" role="alert">
            こちらのレポコは&nbsp;<%= @report.year_of_treatment_end.year %>年&nbsp;<span class="small text-muted">(<%= Time.now.year - @report.year_of_treatment_end.year %>年前)</span>&nbsp;の治療情報です。
          </div>
        <% end %>

        <div class="parent-tab">
          <p class="d-xs-block d-md-none float-right text-success mb-0 px-2" style="font-size:8px">※&nbsp;CL&nbsp;=&nbsp;クリニック(Clinic)</p><br>
          <p class="d-none d-md-block float-right text-success" style="font-size:8px">※&nbsp;CL&nbsp;=&nbsp;クリニック(Clinic)</p>
          <ul class="nav nav-tabs parent-menu">
            <li><a href="#treatment-info" class="active mostleft-tab" data-toggle="tab">レポ</a></li>
            <li><a href="#user-info" data-toggle="tab">ユーザー</a></li>
            <li><a href="#clinic-info" data-toggle="tab">クリニック</a></li>
            <li><a href="#data-analysis" data-toggle="tab">分析</a></li>
          </ul>

          <section class="parent-tab-content" id="data-analysis">
            <div class="child-tab">
              <ul class="nav nav-tabs child-menu">
                <li><a href="#data-summary" class="active" data-toggle="tab">データ</a></li>
              </ul>
              <section class="child-tab-content active" id="data-summary">
                <p class="child-tab-title mb-0 pb-0"><%= @report.clinic.name %>での</p>
                <h4 class="child-tab-title">治療データ分析</h4>
                <div class="box4">
                  &nbsp;<i class="fas fa-pencil-alt"></i>&nbsp;<small class="text-muted"><%= @report.user.name %>さんの&nbsp;<%= @report.clinic.name %>&nbsp;での治療データ分析</small>
                </div>
                <div class="spacer my-0 py-0" style="height:3px;"></div>
                <div class="box27">
                  <span class="box-title">情報</span>
                    <% if @report.str_current_state.present? %>
                      <p class="pt-3 pb-4 small text-muted">※&nbsp;現ステータス:&nbsp;<b><u><%= @report.str_current_state %></u></b>&nbsp;に至った周期の以下情報をもとに分析しています。</p>
                    <% else %>
                      <div class="py-2"></div>
                    <% end %>
                    <ul class="list-unstyled">
                      <li>
                        <% if @report.current_state.present? %>
                          <span class="text-muted small">　　　現状</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_current_state %></span>
                        <% else %>
                          <span class="text-muted small">　　　現状</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.transplant_method.present? %>
                          <span class="text-muted small">　移植方法</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_transplant_method %></span>
                        <% else %>
                          <span class="text-muted small">　移植方法</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.number_of_eggs_collected.present? %>
                          <span class="text-muted small">　採卵個数</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_number_of_eggs_collected %></span>
                        <% else %>
                          <span class="text-muted small">　採卵個数</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.number_of_fertilized_eggs.present? %>
                          <span class="text-muted small">　受精個数</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_number_of_fertilized_eggs %></span>
                        <% else %>
                          <span class="text-muted small">　受精個数</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.number_of_transferable_embryos.present? %>
                          <span class="text-muted small">移植可能数</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_number_of_transferable_embryos %></span>
                        <% else %>
                          <span class="text-muted small">移植可能数</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.total_number_of_transplants.present? %>
                          <span class="text-muted small">　移植回数</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_total_number_of_transplants %></span>
                        <% else %>
                          <span class="text-muted small">　移植回数</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.total_number_of_transplants.present? %>
                          <span class="text-muted small">　移植個数</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_total_number_of_eggs_transplanted %></span>
                        <% else %>
                          <span class="text-muted small">　移植個数</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                      <li>
                        <% if @report.number_of_eggs_stored.present? %>
                          <span class="text-muted small">　貯卵個数</span>&nbsp;：&nbsp;<span class="small"><%= @report.str_number_of_eggs_stored %></span>
                        <% else %>
                          <span class="text-muted small">　貯卵個数</span>&nbsp;：&nbsp;<span class="small">未回答</span>
                        <% end %>
                      </li>
                    </ui>
                </div>
                <div>
                  <p class="small text-muted pt-4"><small>以下は上記情報をもとに算出した結果。</small></p>
                </div>
                <table class="table table-hover p-0">
                  <tr>
                    <td class="result-left-column small" style="width:30%">受精率<i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
                    <td class="pl-3" style="width:70%">
                      <% if @report.number_of_eggs_collected.present? && @report.number_of_eggs_collected <= 50 && @report.number_of_fertilized_eggs.present? && @report.number_of_fertilized_eggs <= 50 %>
                        <% juseiritsu = (@report.number_of_fertilized_eggs.to_f / @report.number_of_eggs_collected.to_f) * 100 %>
                          <% if juseiritsu <= 100.00 %>
                            <span class="result-text"><%= sprintf("%.2f", juseiritsu) %>%<small><br><span class="small text-muted">=&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></small></span>
                          <% else %>
                            <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></span>
                          <% end %>
                      <% else %>
                        <span class="text-muted small">算出不可</span>
                      <% end %>
                    </td>
                  </tr>
                  <tr>
                    <td class="result-left-column small" style="width:30%">移植可能胚到達率<span class="small text-muted">(対 採卵)</span><i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
                    <td class="pl-3" style="width:70%">
                      <% if @report.number_of_eggs_collected.present? && @report.number_of_eggs_collected <= 50 && @report.number_of_transferable_embryos.present? && @report.number_of_transferable_embryos <= 50 %>
                        <% ishokukanouhai_sairan = (@report.number_of_transferable_embryos.to_f / @report.number_of_eggs_collected.to_f) * 100 %>
                        <% if ishokukanouhai_sairan <= 100.00 %>
                          <span class="result-text"><%= sprintf("%.2f", ishokukanouhai_sairan) %>%<small><br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></small></span>
                        <% else %>
                          <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></span>
                        <% end %>
                      <% else %>
                        <span class="text-muted small">算出不可</span>
                      <% end %>
                    </td>
                  </tr>
                  <tr>
                    <td class="result-left-column small" style="width:30%">移植可能胚到達率<span class="small text-muted">(対 受精卵)</span><i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
                    <td class="pl-3" style="width:70%">
                      <% if @report.number_of_fertilized_eggs.present? && @report.number_of_fertilized_eggs <= 50 && @report.number_of_transferable_embryos.present? && @report.number_of_transferable_embryos <= 50 %>
                        <% ishokukanouhai_juseiran = (@report.number_of_transferable_embryos.to_f / @report.number_of_fertilized_eggs.to_f) * 100 %>
                        <% if ishokukanouhai_juseiran <= 100.00 %>
                          <span class="result-text"><%= sprintf("%.2f", ishokukanouhai_juseiran) %>%<small><br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %></span></small></span>
                        <% else %>
                          <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;移植可能胚&nbsp;<%= @report.str_number_of_transferable_embryos %>&nbsp;/&nbsp;受精卵&nbsp;<%= @report.str_number_of_fertilized_eggs %></span></span>
                        <% end %>
                      <% else %>
                        <span class="text-muted small">算出不可</span>
                      <% end %>
                    </td>
                  </tr>
                  <% if @report.current_state.present? && @report.current_state < 5 %>
                    <tr>
                      <td class="result-left-column small" style="width:30%">採卵あたりの妊娠率<i data-feather="bar-chart-2" class="feather-icon-style3 px-1 py-1 pb-2 mx-0"></i></td>
                      <td class="pl-3" style="width:70%">
                        <% if @report.current_state == 1 || @report.current_state == 3 %>
                          <% if @report.number_of_eggs_stored.present? && @report.number_of_eggs_stored <= 50 %>
                            <% number_of_eggs_stored = @report.number_of_eggs_stored %>
                            <% ninshinritsu_sairan = ( 1 / (@report.number_of_eggs_collected - number_of_eggs_stored).to_f) * 100 %>
                              <% if ninshinritsu_sairan <= 100.00 %>
                                <span class="result-text"><%= sprintf("%.2f", ninshinritsu_sairan) %>%<small><br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></small></span>
                              <% else %>
                                <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></span>
                              <% end %>
                          <% else %>
                            <% ninshinritsu_sairan = ( 1 / @report.number_of_eggs_collected.to_f) * 100 %>
                            <% if ninshinritsu_sairan <= 100.00 %>
                              <span class="result-text"><%= sprintf("%.2f", ninshinritsu_sairan) %>%<small><br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></small></span>
                            <% else %>
                              <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></span>
                            <% end %>
                          <% end %>
                        <% elsif @report.current_state == 2 || @report.current_state == 4 %>
                          <% if @report.transplant_method == 3 || @report.transplant_method == 4 %>
                            <% if @report.number_of_eggs_stored.present? && @report.number_of_eggs_stored <= 50 %>
                              <% number_of_eggs_stored = @report.number_of_eggs_stored %>
                              <% ninshinritsu_sairan = ( 2 / (@report.number_of_eggs_collected - number_of_eggs_stored).to_f) * 100 %>
                                <% if ninshinritsu_sairan <= 100.00 %>
                                  <span class="result-text"><%= sprintf("%.2f", ninshinritsu_sairan) %>%<small><br><span class="small text-muted">=&nbsp;2個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></small></span>
                                <% else %>
                                  <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></span>
                                <% end %>
                            <% else %>
                              <% ninshinritsu_sairan = ( 2 / @report.number_of_eggs_collected.to_f) * 100 %>
                              <% if ninshinritsu_sairan <= 100.00 %>
                                <span class="result-text"><%= sprintf("%.2f", ninshinritsu_sairan) %>%<small><br><span class="small text-muted">=&nbsp;2個&nbsp;/&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %></span></small></span>
                              <% else %>
                                <span class="text-muted small">算出不可<br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></span>
                              <% end %>
                            <% end %>
                          <% else %>
                            <% if @report.number_of_eggs_stored.present? && @report.number_of_eggs_stored <= 50 %>
                              <% number_of_eggs_stored = @report.number_of_eggs_stored %>
                              <% ninshinritsu_sairan = ( 1 / (@report.number_of_eggs_collected - number_of_eggs_stored).to_f) * 100 %>
                                <% if ninshinritsu_sairan <= 100.00 %>
                                  <span class="result-text"><%= sprintf("%.2f", ninshinritsu_sairan) %>%<small><br><span class="small text-muted">=&nbsp;1個&nbsp;/&nbsp;(&nbsp;採卵数&nbsp;<%= @report.str_number_of_eggs_collected %>&nbsp;-&nbsp;貯卵数&nbsp;<%= number_of_eggs_stored %>個&nbsp;)</span></small></span>
                                <% else %>
