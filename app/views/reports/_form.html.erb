                        </div>

                        <div class="form-group mt-0 mb-2 pt-0">
                          <p class="question mb-0">採卵までの通院回数</p>
                          <%= f.select :number_of_visits_before_sairan, Report::HASH_TOTAL_NUMBER_OF_TRANSPLANTS.invert, { prompt: "" }, { class: "form-control" }  %>
                          <small class="text-muted"><span class="small">採卵周期開始から採卵日までの(おおよその)通院回数</span></small>
                        </div>

                        <div class="form-group mt-0 mb-2 pt-0">
                          <p class="question mb-0">採卵時の年齢</p>
                          <%= f.select :sairan_age, Report::HASH_SAIRAN_AGE.invert, { prompt: "" }, { class: "form-control" } %>
